@prefix n: <http://faclair.ac.uk/nouns/> .
@prefix a: <http://faclair.ac.uk/adjectives/> .
@prefix v: <http://faclair.ac.uk/verbs/> .
@prefix o: <http://faclair.ac.uk/other/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://faclair.ac.uk/meta/> .

<http://faclair.ac.uk/sources/Watson2001> {

  n:dual_hair
    a :MasculineNoun ;
    rdfs:label "dual" ;
    :sense "a curl, lock or ringlet of hair" , "a plait (in hair, rope etc.)" ;
    :gen "duail" ;
    :pl "dualan" ;
    rdfs:comment "Cf. bachlag, camag" .

  n:dual
    a :MasculineNoun ;
    rdfs:label "dual" ;
    :sense "what might be expected of one because of one’s descendance" , "one’s character considered as inherited" , "a hereditary right" , "a birthright" ;
    :gen "duail" ;
    :pl "dualan" ;
    rdfs:comment "Cf. dualchas , dùthchas" .

  v:bu_dual_dha_sin
    rdfs:label "bu dual dha sin" ;
    :sense "that was to be expected of him (him being the man he is)" ;
    :part n:dual .

  v:bu_dual_dha_baardachd_bho_thaobh_athar
    a :Example ;
    rdfs:label "bu dual dha bàrdachd bho thaobh athar" ;
    :sense "he inherited a talent for poetry from his father’s side of the family" , "literally, poetry was hereditary for him from his father’s side" ;
    :part n:dual .

  n:MacGriogair_aa_Ruadhshruth_dam_bu_dual_bhith_n_Gleann_Liiobhann
    a :Example ;
    rdfs:label "MacGriogair à Ruadhshruth dam bu dual bhith ’n Gleann Lìobhann" ;
    :sense "MacGregor of Roro whose birthright it was to be in Glen Lyon (song)" ;
    :part n:dual .

  a:dualach
    a :Adjective ;
    rdfs:label "dualach" , "dualtach" ;
    :sense "natural" , "inherent" , "innate" , "in one’ nature or temperament (with a suggestion of hereditary influence)" ;
    rdfs:comment "Cf. buailteach" .

  v:tha_e_dualtach_dhut_a_bhith_ag_innse_bhreugan
    a :Example ;
    rdfs:label "tha e dualtach dhut a bhith ag innse bhreugan!" ;
    :sense "it’s in your nature / just like you / typical of you to tell lies!" ;
    :part a:dualach .

  v:tha_iad_dualach_a_bhith_spiiocach
    a :Example ;
    rdfs:label "tha iad dualach a bhith spìocach" ;
    :sense "they are inclined to be stingy / mean" ;
    :part a:dualach .

  v:dualaich_hair
    a :TransitiveVerb ;
    rdfs:label "dualaich" ;
    :sense "curl (hair)" , "twist" , "plait (hair, rope etc.)" ;
    :vn "a’ dualachadh" ;
    rdfs:comment "Cf. bachlaich" .

  n:dualchainnt
    a :FeminineNoun ;
    rdfs:label "dualchainnt" , "dual-chainnt" ;
    :sense "a dialect" ;
    :gen "dualchainnte" ;
    :pl "dualchainntean" .

  n:dualchas
    a :MasculineNoun ;
    rdfs:label "dualchas" ;
    :sense "heritage (especially cultural)" , "inheritance" , "(trad.) one’s character or characteristics (with a suggestion of hereditary influence)" , "one’ inheritance" , "what one is, what might be expected of one, or what one may be entitled to, by reason of one’s descent" ;
    :gen "dualchais" ;
    rdfs:comment "Cf. dual, dùthchas" .

  v:is_e_a_dhualchas_a_bhith_aardanach
    a :Example ;
    rdfs:label "is e a dhualchas a bhith àrdanach" ;
    :sense "it’s in his character / nature to be proud" ;
    :part n:dualchas .

  a:dualchasach
    a :Adjective ;
    rdfs:label "traditional" .

  n:ceool_dualchasach
    a :MasculineNoun ;
    rdfs:label "ceòl dualchasach" ;
    :sense "traditional music" ;
    :part a:dualchasach .






  n:duuthaich
    a :FeminineNoun ;
    rdfs:label "dùthaich" ;
    :sense "a country" , "country" , "countryside" , "a land" , "a territory" , "one’s homeland or home district" ;
    :gen "dùthcha" ;
    :pl "dùthchannan" ;
    rdfs:label "Cf. tìr" .

  n:duuthaich_MhicAoidh
    rdfs:label "dùthaich MhicAoidh" ;
    :sense "the Mackay country / lands (in Sutherland)" ;
    :part n:duuthaich .

  o:O_mo_dhuuthaich_s_tu_th_air_m_aire
    a :Example ;
    rdfs:label "O mo dhùthaich, ’s tu th’ air m’ aire" ;
    :sense "O my homeland, it is you who are on my mind (song)" ;
    :part n:duuthaich .

  n:UUghdarras_Duuthcha_na_h_Alba
    rdfs:label "Ughdarras Dùthcha na h-Alba" ;
    :sense "the Countryside Commission for Scotland" ;
    :part n:duuthaich .

  n:duuthchas
    a :MasculineNoun ;
    rdfs:label "dùthchas" ;
    :sense "the ancestral lands of a clan or an individual (trad.)" , "one’s cultural inheritance or heritage" , "what one is by reason of the place one belongs to" ;
    :gen "dùthchais" ;
    rdfs:comment "Cf. dùthaich, dualchas" .

  v:cha_bhi_duuthchas_aig_mnaoi_no_aig_sagart
    a :Example ;
    rdfs:label "cha bhi dùthchas aig mnaoi no no aig sagart " ;
    :sense "a woman and a priest have no homeland" ;
    :part n:duuthaich .

  a:duuthchasach
    a :Adjective ;
    rdfs:label "dùthchasach" ;
    :sense "native" , "indigenous" , "aboriginal" .




}
