n:dual_hair
  a :MasculineNoun ;
  rdfs:label "dual" ;
  :gen "duail" ;
  :pl "duail" , "dualan" .

v:cuir_an_duail
  rdfs:label "cuir an duail" , "cuir an dualan" ;
  :part n:dual_hair .

v:thig_na_dhualan_bho
  rdfs:label "thig na d(h)ualan (bho ...)" ;
  :sense "issuing in curls (from)" ;
  :part n:dual_hair , v:thig .

v:tuit_na_dhualan_air
  rdfs:label "tuit na d(h)ualan (air ...)" ;
  :sense "fall in curls (upon)" ;
  :part n:dual_hair , v:tuit .

n:dual
  a :MasculineNoun ;
  rdfs:label "dual" ;
  :gen "duail" ;
  :pl "dualan" .

v:is_dual_do
  rdfs:label "is/bu dual (do ...) (rud / rud a dhèanamh)" ;
  :sense "be natural (for someone)" ;
  :part n:dual .

v:bu_dual_dha_sin
  rdfs:label "bu dual dha sin" ;
  :part v:is_dual_do .

v:bu_dual_a_bhith_seinn
  rdfs:label "bu dual a bhith seinn" ;
  :part v:is_dual_do .

v:bu_dual_do_dh_isean_an_rooin_a_dhol_air_a_thooin_chun_na_mara
  rdfs:label "bu dual do dh’isean an ròin a dhol air a thòin chun na mara" ;
  :part v:is_dual_do .

o:mar_as_dual
  rdfs:label "mar as/bu dual" ;
  :part v:is_dual_do .

v:bu_dual_do_rud_bho
  rdfs:label "bu dual do ... rud bho ..." ;
  :sense "inherit something from" ;
  :part v:is_dual_do .

n:dual_fhine
  rdfs:label "dual-fhine" ;
  :part n:dual , n:fine .



